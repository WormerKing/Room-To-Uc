<% content_for(:profile_content) do%>
	<% if @user.role == "player"%>
		<% @user.rooms.each do |room|%>
			<%= link_to room.title,room_path(room.id),class:"btn btn-primary"%>
		<%end%>
	<%elsif @user.role == "janitor"%>
		<% @user.creator_rooms.each do |creator|%>
			<%= link_to creator.title,room_path(creator.id),class:"btn btn-primary"%>
		<%end%>
	<%end%>
<%end%>